{
    "configurations": [
        {
            "name": "InsaneEmscripten",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/../CommonCppIncludes",
                "${workspaceFolder}/../CommonCppLibs/Botan-2.19.3-Emscripten-Llvm-Release/include/botan-2",
                "${workspaceFolder}/Include",
                "${EMSCRIPTEN_SDK}/upstream/emscripten/system/include",
